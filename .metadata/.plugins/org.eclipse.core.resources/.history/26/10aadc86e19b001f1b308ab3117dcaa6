package bbs;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;

public class BbsDAO {
	
	private Connection conn;
	private ResultSet rs;
	
	//접속을 하기위한 생성자
	public BbsDAO() {
		try {
			String dbURL = "";
			String dbID = "";
			String dbPassword = "";
			Class.forName("");
			conn = DriverManager.getConnection(dbURL, dbID, dbPassword);
		}catch(Exception e) {
			e.printStackTrace();
		}
	}

}
